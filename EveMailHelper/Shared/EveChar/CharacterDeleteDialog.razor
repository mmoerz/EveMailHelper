<MudDialog>
    <DialogContent>
        <MudText>Do you really want to delete the character named: @Character.Name</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">No</MudButton>
        <MudButton Color="Color.Primary" OnClick="Delete">Yes</MudButton>
    </DialogActions>
</MudDialog>